<h3><%="Date: #{@out[:date]}\n"%></h3>
<h3>Locations:</h3>
<table>
  <tr>
    <th>id</th>
    <th>lat</th>
    <th>lon</th>
    <th>last update</th>
  </tr>

  <% @out[:locations].each do |location| %>
    <tr>
      <td><%= location[:id] %></td>
      <td><%= location[:lat] %></td>
      <td><%= location[:lon] %></td>
      <td><%= location[:last_update] %></td>
    </tr>
  <% end %>
</table>
